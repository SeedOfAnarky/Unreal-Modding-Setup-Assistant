{
  "name": "unreal-modding-helper",
  "version": "1.0.0",
  "description": "Unreal Modding Helper Application",
  "main": "main.js",
  "author": "SeedOFAnarky",
  "scripts": {
    "start": "webpack --mode development && electron .",
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "package": "electron-builder"
  },
  "dependencies": {
    "axios": "^1.7.8",
    "extract-zip": "^2.0.1",
    "lucide-react": "^0.462.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "win-version-info": "^6.0.1"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "babel-loader": "^9.1.3",
    "electron-builder": "^24.13.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "com.yourcompany.unrealmoddinghelper",
    "productName": "Unreal Modding Helper",
    "files": [
      "dist/bundle.js",
      "main.js",
      "preload.js",
      "public/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "mac": {
      "target": []
    },
    "linux": {
      "target": []
    }
  }
}